<%= simple_form_for(@agent_profile) do |f| %>
  <%= f.error_notification %>

  
    <%= f.input :name %>
    <%= f.input :company_name %>
    <%= f.input :company_address %>
    <%= f.input :company_url %>
    <%= f.input :avatar, :as => :file %>
    <%= f.label :remote_avatar_url, "or image URL" %><br />
    <%= f.text_field :remote_avatar_url %>
    <%= f.input :about %>

    <%= f.submit "Submit", :class => "btn btn-success", :style => "color:white;" %>

<% end %>
